<web-app>
	<display-name>EsiGate</display-name>
	<filter>
		<filter-name>EsiGate</filter-name>
		<filter-class>org.esigate.servlet.ProxyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>EsiGate</filter-name>
