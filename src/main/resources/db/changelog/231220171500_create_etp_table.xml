<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="231220171500" author="Vlad">
        <createTable tableName="etp">
            <column name="id" type="int" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="title" type="varchar(500)" >
                <constraints nullable="false"></constraints>
            </column>
            <column name="target" type="varchar(500)" >
                <constraints nullable="false"></constraints>
            </column>
            <column name="distance_learning_begin_date" type="java.sql.Types.TIMESTAMP">
                <constraints nullable="false"></constraints>
            </column>
            <column name="distance_learning_end_date" type="java.sql.Types.TIMESTAMP">
                <constraints nullable="false"></constraints>
            </column>
            <column name="full_time_learning_begin_date" type="java.sql.Types.TIMESTAMP">
                <constraints nullable="false"></constraints>
            </column>
            <column name="full_time_learning_end_date" type="java.sql.Types.TIMESTAMP">
                <constraints nullable="false"></constraints>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>