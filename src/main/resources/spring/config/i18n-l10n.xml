<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd 
	">

	<util:list id="messages">
		<value>/views/master-page/messages</value>
		<value>/views/misc/messages</value>
		<value>/views/security/messages</value>
		<value>/views/welcome/messages</value>

		<value>/views/route/messages</value>
		<value>/views/routeOffer/messages</value>
		<value>/views/sizePrice/messages</value>
		<value>/views/shipment/messages</value>
		<value>/views/shipmentOffer/messages</value>
		<value>/views/alert/messages</value>
		<value>/views/message/messages</value>
		<value>/views/vehicle/messages</value>
		<value>/views/user/messages</value>
		<value>/views/rating/messages</value>
		<value>/views/complaint/messages</value>
		<value>/views/feepayment/messages</value>
		<value>/views/fundTransferPreference/messages</value>
		
		<value>/views/about/messages</value>
		<value>/views/userGeneralTerms/messages</value>
		<value>/views/passwordRecovery/messages</value>
		<value>/views/userCookies/messages</value>

	</util:list>

</beans>