<?xml version="1.0" encoding="UTF-8"?>
<configuration
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://ch.qos.logback/xml/ns/logback"
        xsi:schemaLocation="http://ch.qos.logback/xml/ns/logback https://files.bram.cloud/file/A3088160-991D-4869-B36F-92FDE92F3B01/direct">

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level [%thread] %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="io.github.blamebutton.breadbox" level="DEBUG"/>
    <logger name="sx.blah.discord" level="INFO"/>

    <root level="WARN">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>