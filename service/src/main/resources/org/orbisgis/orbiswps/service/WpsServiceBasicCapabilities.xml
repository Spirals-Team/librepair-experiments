<wps:Capabilities xmlns:ows="http://www.opengis.net/ows/2.0" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:wps="http://www.opengis.net/wps/2.0" xmlns:orbisgis="http://orbisgis.org" service="WPS" version="2.0.0">
    <ows:ServiceIdentification>
        <ows:Title xml:lang="en">OrbisGIS Local WPS Service</ows:Title>
        <ows:Abstract xml:lang="en">OrbisGIS local instance of the WPS Service</ows:Abstract>
        <ows:Keywords>
            <ows:Keyword xml:lang="en">Toolbox</ows:Keyword>
        </ows:Keywords>
        <ows:Keywords>
            <ows:Keyword xml:lang="en">WPS</ows:Keyword>
        </ows:Keywords>
        <ows:Keywords>
            <ows:Keyword xml:lang="en">OrbisGIS</ows:Keyword>
        </ows:Keywords>
        <ows:ServiceType>WPS</ows:ServiceType>
        <ows:ServiceTypeVersion>2.0.0</ows:ServiceTypeVersion>
        <ows:Fees>NONE</ows:Fees>
        <ows:AccessConstraints>NONE</ows:AccessConstraints>
    </ows:ServiceIdentification>
    <ows:ServiceProvider>
        <ows:ProviderName>OrbisGIS</ows:ProviderName>
        <ows:ProviderSite xlink:href="http://orbisgis.org/"/>
        <ows:ServiceContact>
            <ows:ContactInfo>
                <ows:Address>
                    <ows:ElectronicMailAddress>info_at_orbisgis.org</ows:ElectronicMailAddress>
                </ows:Address>
            </ows:ContactInfo>
        </ows:ServiceContact>
    </ows:ServiceProvider>
    <ows:OperationsMetadata>
        <ows:Operation name="GetCapabilities">
            <ows:DCP>
                <ows:HTTP>
                    <ows:Get xlink:href="http://orbisgis.org/wps"/>
                    <ows:Post xlink:href="http://orbisgis.org/wps"/>
                </ows:HTTP>
            </ows:DCP>
        </ows:Operation>
        <ows:Operation name="DescribeProcess">
            <ows:DCP>
                <ows:HTTP>
                    <ows:Get xlink:href="http://orbisgis.org/wps"/>
                    <ows:Post xlink:href="http://orbisgis.org/wps"/>
                </ows:HTTP>
            </ows:DCP>
        </ows:Operation>
        <ows:Operation name="Execute">
            <ows:DCP>
                <ows:HTTP>
                    <ows:Get xlink:href="http://orbisgis.org/wps"/>
                    <ows:Post xlink:href="http://orbisgis.org/wps"/>
                </ows:HTTP>
            </ows:DCP>
        </ows:Operation>
        <ows:Operation name="GetStatus">
            <ows:DCP>
                <ows:HTTP>
                    <ows:Get xlink:href="http://orbisgis.org/wps"/>
                    <ows:Post xlink:href="http://orbisgis.org/wps"/>
                </ows:HTTP>
            </ows:DCP>
        </ows:Operation>
        <ows:Operation name="GetResult">
            <ows:DCP>
                <ows:HTTP>
                    <ows:Get xlink:href="http://orbisgis.org/wps"/>
                    <ows:Post xlink:href="http://orbisgis.org/wps"/>
                </ows:HTTP>
            </ows:DCP>
        </ows:Operation>
        <ows:Operation name="Dismiss">
            <ows:DCP>
                <ows:HTTP>
                    <ows:Get xlink:href="http://orbisgis.org/wps"/>
                    <ows:Post xlink:href="http://orbisgis.org/wps"/>
                </ows:HTTP>
            </ows:DCP>
        </ows:Operation>
    </ows:OperationsMetadata>
    <ows:Languages>
        <ows:Language>en</ows:Language>
    </ows:Languages>
</wps:Capabilities>