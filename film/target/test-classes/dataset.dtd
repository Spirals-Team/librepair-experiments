<!ELEMENT dataset (
    TICKET*,
    MOVIE*,
    MOVIE_TICKET*)>

<!ELEMENT TICKET EMPTY>
<!ATTLIST TICKET
    ID CDATA #REQUIRED
    TYPE CDATA #IMPLIED
    PRICE CDATA #IMPLIED
    SOLD CDATA #IMPLIED
>

<!ELEMENT MOVIE EMPTY>
<!ATTLIST MOVIE
    ID CDATA #REQUIRED
    TITLE CDATA #REQUIRED
    YEAR CDATA #IMPLIED
    GENRE CDATA #IMPLIED
    DIRECTOR CDATA #IMPLIED
>

<!ELEMENT MOVIE_TICKET EMPTY>
<!ATTLIST MOVIE_TICKET
    TICKETS_ID CDATA #REQUIRED
    MOVIE_ID CDATA #REQUIRED
>
