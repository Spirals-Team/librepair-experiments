<?xml version="1.0" encoding="UTF-8" ?>
<project name="maven-antrun-" default="main"  >
<target name="main">
  <property name="protoc.filename" value="protoc-3.0.0-${os.detected.classifier}.exe"/>
  <property name="protoc.filepath" value="/root/workspace/matsim-org/matsim/385585169/contribs/protobuf/target/${protoc.filename}"/>
  <echo message="Custom Ant Property is: ${protoc.filename}"/>
  <echoproperties/>
  <chmod file="${protoc.filepath}" perm="ugo+rx"/>
  <mkdir dir="/root/workspace/matsim-org/matsim/385585169/contribs/protobuf/target/generated-sources"/>
  <path id="protobuf.input.filepaths.path">
    <fileset dir="/root/workspace/matsim-org/matsim/385585169/contribs/protobuf/src/main/proto">
      <include name="**/*.proto"/>
    </fileset>
  </path>
  <pathconvert property="protobuf.input.filepaths" pathsep=" " refid="protobuf.input.filepaths.path"/>
  <exec failonerror="true" executable="${protoc.filepath}">
    <arg value="-I"/>
    <arg value="/root/workspace/matsim-org/matsim/385585169/contribs/protobuf/src/main/proto"/>
    <arg value="--java_out"/>
    <arg value="/root/workspace/matsim-org/matsim/385585169/contribs/protobuf/target/generated-sources"/>
    <arg line="${protobuf.input.filepaths}"/>
  </exec>
</target>
</project>