                {
                    "range":{
                        "timeStamp":{
                            "gte":%d,
                            "lte":%d,
                            "format":"epoch_millis"
                        }
                    }
                }
            ]
        }
    },
    "aggs":{
        "suggestionFields":{
            "terms":{
                "field":"request.suggestionFields.keyword",
                "size":100,
                "order":{
                    "_count":"desc"
                }
            }
        }
    }
